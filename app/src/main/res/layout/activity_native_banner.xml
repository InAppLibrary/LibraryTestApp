<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_native_banner"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="inova.lk.com.librarytestapp.main.NativeBannerActivity">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:text="@string/page_desc"/>

    <inova.lk.com.inapplibrary.ui.NativeBannerView
        android:id="@+id/nativeBanner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/textView1">

    </inova.lk.com.inapplibrary.ui.NativeBannerView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/nativeBanner"
        android:text="@string/page_desc"/>

</RelativeLayout>
